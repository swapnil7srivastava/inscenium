# Inscenium CMS Environment Configuration

# API Configuration
NEXT_PUBLIC_API_BASE=http://localhost:8080
API_BASE_URL=http://localhost:8080

# Authentication
JWT_SECRET=dev-secret-key
SESSION_SECRET=cms-session-secret

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_REAL_TIME=true
NEXT_PUBLIC_ENABLE_DEBUG=false

# External Services
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_ANALYTICS_ID=

# Environment
NODE_ENV=development
NEXT_PUBLIC_ENVIRONMENT=development

# Database (for server-side components if needed)
DATABASE_URL=postgresql://inscenium:inscenium@localhost:5432/inscenium

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

# Performance
NEXT_PUBLIC_ENABLE_SW=false
NEXT_PUBLIC_ENABLE_OFFLINE=false